<%= render partial: "partials/tab_text", locals: {text: "Log In"} %>

<div class='container'>
	<h3>You should sign in, yo</h3>

	<%= form_for(:session) do |f| %>

      <%= f.label :username %>
      <%= f.text_field :username %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "Sign in" %>
  <% end %>


</div>